    "version": "0.2.0",
    "configurations": [
        {
            "name": "Tests",
            "type": "debugpy",
            "request": "launch",
            "program": "tests/test_unitas.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Start Parser",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "tests"
            ]
        },
        {
            "name": "Start Parser with update",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "--update",
                "tests"
            ]
        },
        {
            "name": "Start Parser with search",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "--url",
                "--search",
                "443",
                "tests"
            ]
        },
        {
            "name": "Start Parser with only service scanned ports",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "--service",
                "tests"
            ]
        },
        {
            "name": "Start Parser with rescan",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "--rescan",
                "tests"
            ]
        },
        {
            "name": "Start Parser with export",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-e",
                "--export",
                "tests"
            ]
        },
        {
            "name": "Start Parser with merger",
            "type": "debugpy",
            "request": "launch",
            "program": "unitas.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "--merge",
                "tests"
            ]
        }
    ]
}